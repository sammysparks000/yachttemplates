[
  {
    "type": 1,
    "title": "Onlook",
    "name": "onlook",
    "description": "Open-source AI-powered visual editor for React apps. Self-hosted with Supabase.",
    "logo": "https://raw.githubusercontent.com/onlook-dev/onlook/main/public/logo.png",
    "image": "onlookdev/onlook:latest",
    "note": "Mount your cloned repo volume (with .env files) to /app. Access at http://YOUR_VPS_IP:3000. Add reverse proxy for HTTPS subdomain.",
    "categories": [
      "Development",
      "Design",
      "AI"
    ],
    "platform": "linux",
    "restart_policy": "unless-stopped",
    "ports": [
      "3000:3000/tcp"
    ],
    "volumes": [
      {
        "container": "/app",
        "bind": "!onlook-data",
        "description": "Path to cloned Onlook repo (include apps/web/client/.env and apps/web/server/.env)"
      }
    ],
    "env": [
      {
        "name": "PUID",
        "label": "PUID",
        "default": "!PUID",
        "description": "User ID for permissions"
      },
      {
        "name": "PGID",
        "label": "PGID",
        "default": "!PGID",
        "description": "Group ID for permissions"
      },
      {
        "name": "TZ",
        "label": "TZ",
        "default": "!TZ",
        "description": "Timezone e.g. America/New_York"
      },
      {
        "name": "NEXTAUTH_URL",
        "label": "NextAuth URL",
        "default": "https://onlook.sammysparks.me",
        "description": "Public URL (must match .env files)"
      }
    ]
  }
]
